{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"/hooks",
		"/screens",
		"/components",
		"/routes/",
		"/middleware/",
		"^../(.*)",
		"^./(.*)",
		"(?=./styles.module.scss)"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
